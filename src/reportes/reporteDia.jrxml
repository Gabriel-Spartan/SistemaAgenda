	<parameter name="usuario_id" class="java.lang.String"/>
	<parameter name="fecha" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
    E.ID_USU_PER,
    U.NOM_USU,
    U.APE_USU,
    E.FEC_EVE,
    E.HOR_EVE,
    E.TIT_EVE,
    E.DES_EVE
FROM EVENTOS E, USUARIOS U  
WHERE E.FEC_EVE = $P{fecha}
AND E.ID_USU_PER = U.ID_USU
  AND E.ID_USU_PER = $P{usuario_id}
ORDER BY E.HOR_EVE;
]]>
